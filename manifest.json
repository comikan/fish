{
  "manifest_version": 3,
  "name": "Fish - Roblox Enhancer",
  "version": "1.0.0",
  "description": "Premium Roblox UI enhancement with pure CSS animations",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*.roblox.com/*"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [{
    "matches": ["*://*.roblox.com/*"],
    "css": ["content/styles.css"],
    "js": ["content/main.js"]
  }]
}
